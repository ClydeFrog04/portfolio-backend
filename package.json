{
  "name": "randal-egan-dot-com",
  "version": "0.0.0",
  "scripts": {
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "ts-node ./src/index.ts",
    "start:dev": "nodemon --require dotenv/config --exec ts-node ./src/index.ts",
    "test": "cross-env DB_ENV=testing jest --watch --coverage"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.6.4",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.27",
    "@types/response-time": "^2.3.4",
    "@types/uuid-1345": "^0.99.21",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "nodemailer": "^6.4.14",
    "pg": "^8.3.3",
    "redis": "^3.0.2",
    "response-time": "^2.3.2",
    "sqlite3": "^5.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "uuid-1345": "^1.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.10",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^26.4.2",
    "knex-cleaner": "^1.3.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
